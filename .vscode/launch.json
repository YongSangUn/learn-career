    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python38",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "envFile": "${workspaceFolder}/.env",
            "stopOnEntry": true
        },
        {
            "name": "Go-lang",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}",
            "env": {},
            "args": [],
            "stopOnEntry": true
        }
    ]
}